{
    "name": "redot/datatables",
    "description": "Datatables for Redot Dashboard",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdelrhman Said",
            "email": "abdelrhmansaidzaki@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Redot\\Datatables\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Redot\\Datatables\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.16",
        "orchestra/testbench": "^9.1",
        "pestphp/pest": "^2.34"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "lint": "vendor/bin/pint",
        "test": "vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Redot\\Datatables\\DatatablesServiceProvider"
            ]
        }
    }
}
